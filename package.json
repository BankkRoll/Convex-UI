  "scripts": {
    "dev": "concurrently \"convex dev\" \"next dev --port 3004\"",
    "build": "pnpm run content:build && pnpm run build:registry && pnpm run build:llms && next build",
    "build:registry": "rimraf public/r && tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && shadcn build ./registry.json -o ./public/r && tsx scripts/clean-registry.ts",
    "build:llms": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-llms-txt.ts",
    "start": "next start",
    "lint": "eslint .",
    "content:build": "contentlayer2 build",
    "clean": "rimraf node_modules .next",
    "typecheck": "contentlayer2 build && tsc --noEmit -p tsconfig.json",
    "format": "prettier --write .",
    "generate:favicons": "tsx scripts/generate-favicons.mjs"
  },
