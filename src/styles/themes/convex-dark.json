{
  "name": "convex-dark",
  "displayName": "Convex Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#121212",
    "editor.foreground": "#ffffff"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#7e7e7e",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "constant.language"
      ],
      "settings": {
        "foreground": "#3ecf8e"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name",
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#3ecf8e"
      }
    },
    {
      "scope": ["variable", "support.variable", "variable.parameter"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#bda4ff"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#ffcda1"
      }
    },
    {
      "scope": [
        "support.function",
        "entity.name.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#3ecf8e"
      }
    },
    {
      "scope": [
        "punctuation",
        "punctuation.separator",
        "meta.brace",
        "punctuation.definition"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["meta.tag", "entity.name.tag"],
      "settings": {
        "foreground": "#3ecf8e"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#3ecf8e"
      }
    },
    {
      "scope": ["meta.import", "string.quoted.single", "string.quoted.double"],
      "settings": {
        "foreground": "#ffcda1"
      }
    }
  ]
}
