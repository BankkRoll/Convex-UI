{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "realtime-avatar-stack-vite",
  "type": "registry:component",
  "title": "Realtime Avatar Stack (Vite)",
  "description": "Display a stack of avatars for Vite showing users currently present in a room. Updates in realtime as users join and leave. Includes full Convex backend with presence tracking.",
  "registryDependencies": ["avatar", "tooltip"],
  "dependencies": ["convex@latest"],
  "files": [
    {
      "path": "src/registry/convex/blocks/realtime-avatar-stack-vite/components/avatar-stack.tsx",
      "type": "registry:component"
    },
    {
      "path": "src/registry/convex/blocks/realtime-avatar-stack-vite/components/realtime-avatar-stack.tsx",
      "type": "registry:component"
    },
    {
      "path": "src/registry/convex/blocks/realtime-avatar-stack-vite/hooks/use-realtime-presence-room.ts",
      "type": "registry:hook"
    },
    {
      "path": "src/registry/convex/blocks/realtime-avatar-stack-vite/convex/schema.ts",
      "type": "registry:file",
      "target": "convex/schema.ts"
    },
    {
      "path": "src/registry/convex/blocks/realtime-avatar-stack-vite/convex/presence.ts",
      "type": "registry:file",
      "target": "convex/presence.ts"
    }
  ],
  "envVars": {
    "CONVEX_DEPLOYMENT": "",
    "VITE_CONVEX_URL": ""
  }
}
